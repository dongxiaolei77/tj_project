    <entry>
      <name>初始化存量数据</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>update dw_risk.risk_cust_perrisk_info t1 
inner join
(
	select  cert_no,'风险预警' as ys_result, group_concat(asso_risk_type) as risk_dtl  from 
	(
		select t1.* from 
		(
			select  
			distinct cert_no,
			case when asso_risk_type='01-客户和反担保人（可以识别亲属关系）均为在保在审客户'  then '客户和亲属均为在保在审客户' 
			when asso_risk_type='02-在保在审客户互为反担保人'  then '在保在审客户互为反担保人'  
			end as asso_risk_type 
			from dw_base.dwd_guar_asso_info_tmp 
			where asso_risk_type in ('01-客户和反担保人（可以识别亲属关系）均为在保在审客户','02-在保在审客户互为反担保人')
			) t1 inner join 
			(
			select distinct  substring_index(_from,'/',-1) as cert_no  
			from  risk_asso_info  where asso_type='担保关系' 
			) t2 on t1.cert_no=t2.cert_no
	) a group by cert_no
) t2 on t1.per_cust_id=t2.cert_no 
set t1.ys_result=t2.ys_result, t1.risk_dtl=t2.risk_dtl;
commit;

update dw_risk.risk_cust_perrisk_info set ys_result='风险预警',risk_dtl='客户和亲属均为在保在审客户,在保在审客户互为反担保人' 
where per_cust_id ='370783198512011782';
commit;</sql>
