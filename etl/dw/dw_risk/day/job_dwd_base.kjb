      <sql>delete  from  dw_base.dwd_counter_guar_info where data_source='档案室-线下';
commit;
-- 加工存量反担保关系
insert into dw_base.dwd_counter_guar_info 
select
distinct 
t1.cert_no as cust_id,-- 客户id
t1.cust_name as cust_name,
t1.cert_no,
case when t3.code is null then '企业' else '自然人' end  as cust_type,
t1.guar_id,-- 项目编号
null as counter_guar_id, -- 反担保合同编号
case when t2.loan_notify_no='' then null else t2.loan_notify_no end as notify_code, -- 放款通知书编号
case when t2.item_stt in ('退审','待放款','待签约','提报中','审批中','待出函','已出函','已批复') or item_stt is null  then '在审' 
when  t2.item_stt='已放款' then '在保' when  t2.is_compensate=1 then '代偿' when t2.item_stt='已解保' and t2.is_compensate=0 then '解保' end  as item_stt,-- 项目状态
'连带责任保证' as guar_asso,-- 保证担保方式
t1.counter_guar_certno,-- 反担保人id（身份证号）
t1.counter_guar_name,-- 反担保人姓名
'自然人' as counter_guar_type,-- 反担保主体类型
null as ct_guar_person_phone, -- 反担保人手机号码,
null as ct_guar_person_addr, -- 反担保人送达地址,
null as legal_person_name, -- 法人名称,
null as legal_person_certno, -- 法人身份证号,
'亲属关系' as asso_type, -- 与客户的关系类型
t1.counter_ref as asso_des,-- 关系描述
t2.loan_amt*1 as counter_loan_amt,-- 反担保金额（万元）
t2.loan_amt as loan_amt,-- 借款合同金额（万元）
'档案室-线下'  as data_source,-- 数据来源
curdate() as create_time,-- 创建时间
curdate() as update_time-- 更新时间
from (
	select distinct t1.guar_id,cust_name,cert_no,t2.counter_guar_certno,t1.counter_guar_name,
	'父母'  as counter_ref from  
	(select distinct * from  dw_base.dwd_cust_danbao_un_struct_result) t1 
	inner  join 
	(select  distinct  debt_name,debt_certno , counter_guar_name,counter_guar_certno  from  dw_nd.ods_imp_das_counter_guar_info) t2 
	on t1.cert_no=t2.debt_certno and t1.counter_guar_name=t2.counter_guar_name where t1.counter_ref in ('父母','父亲','母亲')
	union all 
	select t1.guar_id,cust_name,cert_no,t2.counter_guar_certno,t1.counter_guar_name,
	'兄弟或姊妹' as counter_ref from  
	(select distinct * from  dw_base.dwd_cust_danbao_un_struct_result) t1 
	inner  join 
	(select  distinct  debt_name,debt_certno , counter_guar_name,counter_guar_certno  from  dw_nd.ods_imp_das_counter_guar_info) t2 
	on t1.cert_no=t2.debt_certno and t1.counter_guar_name=t2.counter_guar_name where t1.counter_ref in ('兄弟','兄弟姊妹','姐姐','哥哥','弟弟','妹妹')
  union all 
  select t1.guar_id,cust_name,cert_no,t2.counter_guar_certno,t1.counter_guar_name,
	'配偶' as counter_ref from  
	(select distinct * from  dw_base.dwd_cust_danbao_un_struct_result) t1 
	inner  join 
	(select  distinct  debt_name,debt_certno , counter_guar_name,counter_guar_certno  from  dw_nd.ods_imp_das_counter_guar_info) t2 
	on t1.cert_no=t2.debt_certno and t1.counter_guar_name=t2.counter_guar_name where t1.counter_ref in ('配偶','妻子','丈夫')
  union all 
  select t1.guar_id,cust_name,cert_no,t2.counter_guar_certno,t1.counter_guar_name,
	'子女' as counter_ref from  
	(select distinct * from  dw_base.dwd_cust_danbao_un_struct_result) t1 
	inner  join 
	(select  distinct  debt_name,debt_certno , counter_guar_name,counter_guar_certno  from  dw_nd.ods_imp_das_counter_guar_info) t2 
	on t1.cert_no=t2.debt_certno and t1.counter_guar_name=t2.counter_guar_name where t1.counter_ref in ('儿子','女儿')
  union all
  select t1.guar_id,cust_name,cert_no,t2.counter_guar_certno,t1.counter_guar_name,
	'非家庭成员' as counter_ref from  
	(select distinct * from  dw_base.dwd_cust_danbao_un_struct_result) t1 
	inner  join 
	(select  distinct  debt_name,debt_certno , counter_guar_name,counter_guar_certno  from  dw_nd.ods_imp_das_counter_guar_info) t2 
	on t1.cert_no=t2.debt_certno and t1.counter_guar_name=t2.counter_guar_name where t1.counter_ref in ('侄子','妹夫','姐夫','朋友','法人配偶')
) t1 left join (select  *   from  dw_base.dwd_guar_info_all) t2
on t1.guar_id=t2.guar_id
left join (select   distinct left(province_code,2) as code from  dw_base.dim_chinese_district_code) t3
on left(t1.cert_no,2)=t3.code;
commit;
delete  from  dw_base.dwd_counter_guar_info where counter_guar_certno  in ('370723196309025569') and data_source='档案室-线下';
commit;
delete  from  dw_base.dwd_counter_guar_info where counter_guar_certno=cust_id and data_source='档案室-线下';
commit;
