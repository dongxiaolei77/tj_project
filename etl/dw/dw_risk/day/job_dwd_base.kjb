      <name>加工增量反担保人信息-客户直通</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>delete  from  dw_base.dwd_counter_guar_info where data_source='客户直通';
commit;
-- 加工增量反担保关系
insert into dw_base.dwd_counter_guar_info 
select * from (
select 
distinct
t1.cust_id_no as cust_id, 
t1.cust_name,
t1.cust_id_no  as cert_no,
case when t4.code is null then '企业' else '自然人' end  as cust_type,
t3.guar_id,
null as counter_guar_id, -- 反担保合同编号
case when t3.loan_notify_no='' then null else t3.loan_notify_no end as notify_code, -- 放款通知书编号
case when t3.item_stt in ('退审','待放款','待签约','提报中','审批中','待出函','已出函','已批复') or item_stt is null  then '在审' 
when  t3.item_stt='已放款' then '在保' when  t3.is_compensate=1 then '代偿' when t3.item_stt='已解保' and t3.is_compensate=0 then '解保' end  as item_stt,-- 项目状态
'连带责任保证' as guar_asso,-- 保证担保方式
replace(replace(trim(t2.id_no),'=',''),'/','') as counter_guar_certno,-- 反担保人id（身份证号）
replace(t2.counter_name,'l','') as counter_guar_name,-- 反担保人姓名
case when t5.code is null then '企业' else '自然人' end as counter_guar_type,-- 反担保主体类型
null as ct_guar_person_phone, -- 反担保人手机号码,
null as ct_guar_person_addr, -- 反担保人送达地址,
null as legal_person_name, -- 法人名称,
null as legal_person_certno, -- 法人身份证号,
'亲属关系' as asso_type, -- 与客户的关系类型
case  when counter_type='1' then '配偶' when counter_type='2' then '父母' when counter_type='3' then '子女' when counter_type='10' then '股东' when counter_type='11' then '法定代表人' when counter_type='12' then '实际控制人'
else '非家庭成员' end  as asso_des,-- 关系描述
t3.loan_amt*1 as counter_loan_amt,-- 反担保金额（万元）
t3.loan_amt as loan_amt,-- 借款合同金额（万元）
'客户直通'  as data_source,-- 数据来源
t2.create_time as create_time,-- 创建时间
t2.update_time as update_time-- 更新时间
from 
(
		select * from 
		(
				select * from dw_nd.ods_bizhall_guar_apply 
				where product_code in  ('PA0080503') and guar_id is not null 
        and apply_code is not null  
				order by update_time desc
		) a   group by a.guar_id,a.cust_id_no
) t1 inner join 
(
		select *  from 
		(
				select  *  from dw_nd.ods_bizhall_guar_apply_counter 
        where id_no is not null and apply_code is not null and  counter_name is not null  
				order by update_time desc
		) a  group by a.apply_code,a.id_no
) t2 
on t1.apply_code=t2.apply_code
left join 
(
		select  t1.*,t2.* from 
		(
				select  a.id,a.code from (
				select id,code from dw_nd.ods_t_biz_project_main 
				where proj_origin in ('05') order by update_time desc) a group by a.id 
		) t1 inner join 
		(
				select  *   from  dw_base.dwd_guar_info_all where item_stt  in ('退审','待放款','待签约','提报中','审批中','待出函','已出函','已批复','已放款','已解保','已代偿')
		) t2
		on t1.code=t2.guar_id
) t3
on t1.guar_id=t3.id
left join (select   distinct left(province_code,2) as code from  dw_base.dim_chinese_district_code) t4
on left(t1.cust_id_no,2)=t4.code
left join (select   distinct left(province_code,2) as code from  dw_base.dim_chinese_district_code) t5
on left(replace(replace(trim(t2.id_no),'=',''),'/',''),2)=t5.code
where t3.guar_id is not null  and t1.cust_id_no&lt;&gt;replace(replace(trim(t2.id_no),'=',''),'/','')
group by t1.cust_id_no,t3.guar_id,replace(replace(trim(t2.id_no),'=',''),'/','') having count(*)=1
) a where length(counter_guar_certno)&lt;=20 and counter_guar_certno&lt;&gt;'370634198806090022';
commit;</sql>
