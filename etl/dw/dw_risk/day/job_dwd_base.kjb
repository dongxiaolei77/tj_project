commit;
-- 加工增量反担保关系
insert into dw_base.dwd_counter_guar_info 
select 
distinct
t3.cert_no  as cust_id, 
t3.cust_name,
t3.cert_no,
case when t4.code is null then '企业' else '自然人' end  as cust_type,
t3.guar_id,
null as counter_guar_id, -- 反担保合同编号
case when t3.loan_notify_no='' then null else t3.loan_notify_no end as notify_code, -- 放款通知书编号
case when t3.item_stt in ('退审','待放款','待签约','提报中','审批中','待出函','已出函','已批复') or item_stt is null  then '在审' 
when  t3.item_stt='已放款' then '在保' when  t3.is_compensate=1 then '代偿' when t3.item_stt='已解保' and t3.is_compensate=0 then '解保' end  as item_stt,-- 项目状态
'连带责任保证' as guar_asso,-- 保证担保方式
t1.ct_guar_person_id_no as counter_guar_certno,-- 反担保人id（身份证号）
t1.ct_guar_person_name as counter_guar_name,-- 反担保人名称
case when t1.ct_guar_person_main_type='01' then '自然人' else '企业' end  as counter_guar_type,-- 反担保主体类型
t1.ct_guar_person_phone, -- 反担保人手机号码,
t1.ct_guar_person_addr, -- 反担保人送达地址,
t1.legal_person_name, -- 法人名称,
t1.legal_person_id_no as legal_person_certno, -- 法人身份证号,
'亲属关系' as asso_type, -- 与客户的关系类型
case  when ct_guar_relation='02' then '配偶' when ct_guar_relation='03' then '子女' when ct_guar_relation='04' then '父母'
when ct_guar_relation='05' then '儿媳或女婿' when ct_guar_relation='06' then '岳父母' when ct_guar_relation ='07' then '兄弟或姊妹' 
else '非家庭成员' end  as asso_des,-- 关系描述
t3.loan_amt*1 as counter_loan_amt,-- 反担保金额（万元）
t3.loan_amt as loan_amt,-- 借款合同金额（万元）
'线上签约'  as data_source,-- 数据来源
t1.create_time as create_time,-- 创建时间
t1.update_time as update_time-- 更新时间
from 
(
  select  *  from 
  (
		  select  t1.* from 
      (
				  select  *   from dw_nd.ods_t_ct_guar_person 
				  where ct_guar_person_name is not null 
				  and ct_guar_person_id_no is not null 
		  ) t1 inner join 
		  (
				  select  project_id,max(data_type) as data_type   from dw_nd.ods_t_ct_guar_person 
				  where ct_guar_person_name is not null and ct_guar_person_id_no is not null 
				  group by project_id
      ) t2
		  on t1.project_id=t2.project_id and t1.data_type=t2.data_type
		  order by create_time desc
  ) a group by a.project_id,a.ct_guar_person_id_no
) t1
inner join 
(
		select  t1.*,t2.* from 
		(
				select  a.id,a.code from (
				select id,code from dw_nd.ods_t_biz_project_main 
				order by update_time desc) a group by a.id 
		) t1 inner join 
		(
				select  *   from  dw_base.dwd_guar_info_all where item_stt in 
				('退审','待放款','待签约','提报中','审批中','待出函','已出函','已批复','已放款','已解保','已代偿')
		) t2
		on t1.code=t2.guar_id
) t3
on t1.project_id=t3.id
left join (select   distinct left(province_code,2) as code from  dw_base.dim_chinese_district_code) t4
on left(t3.cert_no,2)=t4.code
where t3.guar_id is not null  and t3.cert_no&lt;&gt;t1.ct_guar_person_id_no
group by t3.cert_no,t3.guar_id,t1.ct_guar_person_id_no having count(*)=1;
commit;</sql>
