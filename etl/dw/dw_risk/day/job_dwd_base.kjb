t1.counter_guar_certno,-- 反担保人id（身份证号）
t1.counter_guar_name,-- 反担保人姓名
'自然人' as counter_guar_type,-- 反担保主体类型
null as ct_guar_person_phone, -- 反担保人手机号码,
null as ct_guar_person_addr, -- 反担保人送达地址,
null as legal_person_name, -- 法人名称,
null as legal_person_certno, -- 法人身份证号,
'亲属关系' as asso_type, -- 与客户的关系类型
case when asso_des='配偶' then '配偶' when asso_des='父母' then '子女' end  as asso_des,-- 关系描述
t2.loan_amt*1 as counter_loan_amt,-- 反担保金额（万元）
t2.loan_amt as loan_amt,-- 借款合同金额（万元）
'档案室-线下'  as data_source,-- 数据来源
curdate() as create_time,-- 创建时间
curdate() as update_time-- 更新时间

  from  dw_nd.ods_imp_das_counter_guar_info t1 inner join (
select  cert_no ,item_stt,guar_id,asso_des,loan_amt,loan_notify_no,is_compensate from (
select  t1.is_compensate,t1.cert_no,t1.item_stt,t1.loan_amt,t1.loan_notify_no,t1.guar_id,t2.asso_des   from  dw_base.dwd_guar_info_all t1 inner join 
(
select distinct  counter_guar_certno,asso_des from  dw_base.dwd_counter_guar_info where counter_guar_certno not in 
	(
	select distinct cust_id from dw_base.dwd_counter_guar_info
	)
) t2 on t1.cert_no=t2.counter_guar_certno
where  item_stt not in ('不受理','已关闭','已终止','手工关闭','已否决') ) a  group by cert_no having count(guar_id)
=1
) t2 on t1.debt_certno=t2.cert_no;
commit;
</sql>
