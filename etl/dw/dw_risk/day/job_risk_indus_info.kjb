    <entry>
      <name>插入评级结果</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>

-- 插入行业评级
insert into dw_risk.indus_rate_rlt_day(code,c_rate_flag_1,c_rate_flag_2,c_rate_flag_3,a_rate_flag,rate_flag,version,create_time,update_time)
select 
code,
c_rate_flag_1,
c_rate_flag_2,
c_rate_flag_3,

a_rate_flag,

case when a_rate_flag = 1 then 'A'
		 when (c_rate_flag_1 + c_rate_flag_2 + c_rate_flag_3) >= 1 then 'C'
		 else 'B' end as rate_flag,
'v_20210623' as version,
curdate() as create_time,
curdate() as update_time

from 
(
select 

code,
econ_class,

### C级策略1，近一年成功放贷业务
case when (date(recent_loan_dt) &lt;= date_sub(current_date(), interval '1' YEAR) or recent_loan_dt is null )
then 1 else 0 end as c_rate_flag_1,

### C级策略2，成功贷款单数&lt; 10单
case when date(recent_loan_dt) > date_sub(current_date(), interval '1' YEAR)
and (end_guar_num + under_guar_num) &lt; 10 then 1 else 0 end as c_rate_flag_2,

### C级策略3，成功贷款单数> 100单，且代偿率>0.1%或逾期>1%
case when 
date(recent_loan_dt) > date_sub(current_date(), interval '1' YEAR)
and
(end_guar_num + under_guar_num) > 100 
and 
(
(compt_amt / if(end_guar_amt=0,1,end_guar_amt))  > 0.001
or (warn_amt / if(under_guar_amt=0,1,under_guar_amt))  > 0.001
or (vf_ovdue_amt / if(under_guar_amt=0,1,under_guar_amt)) > 0.01 	
)
then 1 else 0 end as c_rate_flag_3,

### A级策略，成功贷款单数> 100单，且代偿率&lt;0.1%且逾期&lt;1%
case when 
under_guar_num > 100 
and 
((compt_amt / if(end_guar_amt=0,1,end_guar_amt)) &lt; 0.001 or compt_amt is null)
and ((warn_amt / if(under_guar_amt=0,1,under_guar_amt)) &lt; 0.001 or warn_amt is null)
and ((vf_ovdue_amt / if(under_guar_amt=0,1,under_guar_amt)) &lt; 0.01 	or vf_ovdue_amt is null)

then 1 else 0 end as a_rate_flag


from 

(
select 
code,
econ_class,
substring_index(econ_class,'/',1) hy_1,
replace(substring_index(econ_class,'/',2),concat(substring_index(econ_class,'/',1),'/'),'') hy_2,
replace(substring_index(econ_class,'/',3),concat(substring_index(econ_class,'/',2),'/'),'') hy_3,
replace(substring_index(econ_class,'/',4),concat(substring_index(econ_class,'/',3),'/'),'') hy_4,
recent_loan_dt,
early_loan_dt,
reject_guar_num,
reject_apply_amt,
end_guar_num,
end_guar_amt,
under_guar_num,
under_guar_amt,
under_aprv_guar_num,
under_aprv_guar_amt,
compt_num,
compt_amt,
warn_num,
warn_amt,
vf_ovdue_num,
vf_ovdue_amt
-- select *
from 
dw_risk.indus_rate_detail_info_day
) hy_base

-- order by 2

) tmp;
commit;

-- 插入产业集群评级

insert into dw_risk.clus_scheme_rate_rlt_day(scheme_code,c_rate_flag_1,c_rate_flag_2,c_rate_flag_3,a_rate_flag,rate_flag,version,create_time,update_time)

select 
scheme_code,
c_rate_flag_1,
c_rate_flag_2,
c_rate_flag_3,

a_rate_flag,

case when a_rate_flag = 1 then 'A'
		 when (c_rate_flag_1 + c_rate_flag_2 + c_rate_flag_3) >= 1 then 'C'
		 else 'B' end as rate_flag,
'v_20210623' as version,
curdate() as create_time,
curdate() as update_time

from 
(

select 
scheme_code,

### C级策略1，近一年成功放贷业务
case when (date(recent_loan_dt) &lt;= date_sub(current_date(), interval '1' YEAR) or recent_loan_dt is null )
then 1 else 0 end as c_rate_flag_1,

### C级策略2，成功贷款单数&lt; 10单
case when 
date(recent_loan_dt) > date_sub(current_date(), interval '1' YEAR)
and (end_guar_num + under_guar_num) &lt; 10 then 1 else 0 end as c_rate_flag_2,

### C级策略3，成功贷款单数> 100单，且代偿率/逾期/预警
case when 
date(recent_loan_dt) > date_sub(current_date(), interval '1' YEAR)
and
(end_guar_num + under_guar_num) > 100 
and 
(
compt_amt > 0 or
warn_amt > 0 or
vf_ovdue_amt > 0	
)
then 1 else 0 end as c_rate_flag_3,

### A级策略，成功贷款单数> 100单，且无代偿/逾期/预警
case when 
under_guar_num > 100 
and 
(
(compt_amt  = 0 or compt_amt is null )
and
(warn_amt  = 0 or warn_amt is null )
and
(vf_ovdue_amt = 0 or vf_ovdue_amt is null)
)
then 1 else 0 end as a_rate_flag


from 

dw_risk.clus_scheme_rate_detail_info_day
) tmp;
commit;
</sql>
