		asso_type,'父母'  as asso_des,'1' as asso_sts,null as asso_guar_sts
		from   
		( 
				select cust_id,cust_name,cust_type,counter_guar_certno,counter_guar_name,counter_guar_type,asso_type,asso_des from
				dw_base.dwd_counter_guar_info 
				where asso_type='亲属关系' and counter_guar_type='自然人' and asso_des in ('子女') 
				union 
				select cust_id,cust_name,cust_type,legal_person_certno,legal_person_name,'自然人' as counter_guar_type,asso_type,asso_des
				from dw_base.dwd_counter_guar_info where asso_type='亲属关系' and counter_guar_type='企业' 
				and legal_person_certno is not null and asso_des in ('子女') 
		) a
		union 
		select  concat('risk_obj_idx','/',cust_id) as _from , 
		cust_name as _from_name,
		cust_type as _from_type,
		concat('risk_obj_idx','/',counter_guar_certno) as _to,
		counter_guar_name as _to_name,
		counter_guar_type as _to_type,
		asso_type,'子女' as asso_des,'1' as asso_sts,null as asso_guar_sts
		from 
		( 
				select cust_id,cust_name,cust_type,counter_guar_certno,counter_guar_name,counter_guar_type,asso_type,asso_des from
				dw_base.dwd_counter_guar_info 
				where asso_type='亲属关系' and counter_guar_type='自然人' and asso_des in ('父母') 
				union 
				select cust_id,cust_name,cust_type,legal_person_certno,legal_person_name,'自然人' as counter_guar_type,asso_type,asso_des
				from dw_base.dwd_counter_guar_info where asso_type='亲属关系' and counter_guar_type='企业' 
				and legal_person_certno is not null and asso_des in ('父母') 
		) a
		union 
		select  concat('risk_obj_idx','/',cust_id) as _from , 
		cust_name as _from_name,
		cust_type as _from_type,
		concat('risk_obj_idx','/',counter_guar_certno) as _to,
		counter_guar_name as _to_name,
		counter_guar_type as _to_type,
		asso_type,'岳父母或公婆' as asso_des,'1' as asso_sts,null as asso_guar_sts
		from 
		( 
				select cust_id,cust_name,cust_type,counter_guar_certno,counter_guar_name,counter_guar_type,asso_type,asso_des from
				dw_base.dwd_counter_guar_info 
				where asso_type='亲属关系' and counter_guar_type='自然人' and asso_des in ('儿媳或女婿') 
				union 
				select cust_id,cust_name,cust_type,legal_person_certno,legal_person_name,'自然人' as counter_guar_type,asso_type,asso_des
				from dw_base.dwd_counter_guar_info where asso_type='亲属关系' and counter_guar_type='企业' 
				and legal_person_certno is not null and asso_des in ('儿媳或女婿') 
		) a
		union 
		select  concat('risk_obj_idx','/',cust_id) as _from , 
