    <sql>select 
_from,_from_name,_from_type,_to,_to_name,_to_type,asso_type,asso_des,
1  as asso_sts,
null asso_guar_sts,
null as begin_dt,
null as end_dt,
curdate() as create_time,
curdate() as update_time
from 
(
		select  
		distinct
		concat('risk_obj_idx','/',counter_guar_certno) as _from , 
		counter_guar_name as _from_name,
		counter_guar_type as _from_type,
		concat('risk_obj_idx','/',cust_id) as _to,
		cust_name as _to_name,
		cust_type as _to_type,
		asso_type,
		asso_des
		from
		( 
				select counter_guar_certno,counter_guar_name,counter_guar_type,cust_id,cust_name,cust_type,asso_type,asso_des
				from dw_base.dwd_counter_guar_info where asso_type='亲属关系' and counter_guar_type='自然人' 
				union 
				select legal_person_certno,legal_person_name,'自然人' as counter_guar_type,cust_id,cust_name,cust_type,asso_type,asso_des
				from dw_base.dwd_counter_guar_info where asso_type='亲属关系' and counter_guar_type='企业' and  legal_person_certno is not null
		) a
        where counter_guar_certno &lt;&gt; '371323198904052148'
		group by cust_id,counter_guar_certno
		union
		select  
		distinct
		concat('risk_obj_idx','/',cust_id) as _from , 
		cust_name as _from_name,
		cust_type as _from_type,
		concat('risk_obj_idx','/',counter_guar_certno) as _to,
		counter_guar_name as _to_name,
		counter_guar_type as _to_type,
		asso_type,
		asso_des
		from
		( 
				select cust_id,cust_name,cust_type,counter_guar_certno,counter_guar_name,counter_guar_type,asso_type,asso_des from
				dw_base.dwd_counter_guar_info 
				where asso_type='亲属关系' and counter_guar_type='自然人' and asso_des in ('配偶','兄弟或姊妹','非家庭成员') 
				union 
				select cust_id,cust_name,cust_type,legal_person_certno,legal_person_name,'自然人' as counter_guar_type,asso_type,asso_des
				from dw_base.dwd_counter_guar_info where asso_type='亲属关系' and counter_guar_type='企业' 
				and legal_person_certno is not null and asso_des in ('配偶','兄弟或姊妹','非家庭成员') 
		) a group by cust_id,counter_guar_certno
) a
union 
select  a.*,
null as begin_dt,
null as end_dt,
curdate() as create_time,
curdate() as update_time   
from  
(
  -- 儿子，女儿，儿媳，女婿，父亲，母亲，岳父，岳母
		select  concat('risk_obj_idx','/',cust_id) as _from , 
		cust_name as _from_name,
		cust_type as _from_type,
		concat('risk_obj_idx','/',counter_guar_certno) as _to,
		counter_guar_name as _to_name,
