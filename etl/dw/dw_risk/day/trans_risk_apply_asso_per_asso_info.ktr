    <sql>select 
_from,_from_name,_from_type,_to,_to_name,_to_type,asso_type,asso_des,
case when asso_sts like '%在保%' then '在保' 
		 when asso_sts like '%在审%' then '在审'
     when asso_sts not like '%在审%' and asso_sts not like '%在保%' then '解保'
     end  as asso_guar_sts,
curdate() as create_time,
curdate() as update_time
from (
select  
distinct
concat('risk_obj_idx','/',cust_id) as _from , 
cust_name as _from_name,
cust_type as _from_type,
concat('risk_obj_idx','/',counter_guar_certno) as _to,
counter_guar_name as _to_name,
counter_guar_type as _to_type,
'担保关系' as asso_type,
concat(counter_guar_name,'给',cust_name,'提供反担保') as asso_des,
group_concat(item_stt) as asso_sts
from dw_base.dwd_counter_guar_info group by cust_id,counter_guar_certno
) a order by _from,_to
</sql>
