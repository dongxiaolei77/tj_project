    <sql>select  *  from  (
select  
distinct
concat('risk_obj_idx','/',cust_id) as _from , 
concat('risk_obj_idx','/',counter_guar_certno) as _to,
min(loan_dt) as begin_dt,
max(due_dt) as end_dt,
case when  date_format(t2.due_dt,'%Y%m%d')>=date_format(curdate(),'%Y%m%d')
 then '1' else '0' end  as asso_sts
from  dw_base.dwd_counter_guar_info t1 
left join 
(
	select   t1.guar_id,-- 项目编号
  coalesce(coalesce(case when t1.loan_begin_dt='' then null else date_format(t1.loan_begin_dt,'%Y-%m-%d') end ,case when notify_dt='' then null else date_format(notify_dt,'%Y-%m-%d') end ),case when t1.loan_notify_dt='' then null else date_format(t1.loan_notify_dt,'%Y-%m-%d') end )
  as loan_dt,-- 借款日期
  coalesce(date_format(t1.loan_end_dt,'%Y-%m-%d'),case when t1.due_dt='' then null else date_format(t1.due_dt,'%Y-%m-%d') end) as due_dt-- 责任到期日
  from dw_base.dwd_guar_info_all t1
) t2 on t1.guar_id=t2.guar_id
group by cust_id,counter_guar_certno
) a order by _from,_to</sql>
