    <connection>dw_base</connection>
    <sql>select
distinct
mgr_id as bank_mgr_id,
ifnull(case when mgr_name='' then null else mgr_name end,'暂无') as bank_mgr_name,
ifnull(cert_no,'暂无') as bank_mgr_certno,
ifnull(tel_no,'暂无') as bank_mgr_telno,
ifnull(dept_name,'暂无') as bank_name,
case when dept_name like '%农村商业%' or dept_name like '%农商%' then '农商银行'
when dept_name like '%邮储%' or dept_name like '%邮政储蓄%' then '邮储银行'
when dept_name like '%农业%' or dept_name like '%农行%' then '农业银行'
else '其他银行' end as bank_class,
org_id as region_code,
concat(t2.city_name,'市',area_name) as region_name,
date_format(create_dt,'%Y-%m-%d') as reg_dt,
curdate() as create_time,
curdate() as update_time
from  dw_base.dwd_mgr_info t1
left join 
(select distinct  city_name,area_id,area_name  from  dw_nd.ods_imp_area_map) t2
on t1.org_id=t2.area_id
where dept_name is not null 
union
select
distinct 
mgr_id as bank_mgr_id,
mgr_name as bank_mgr_name,
cert_no as bank_mgr_certno,
tel_no as bank_mgr_telno,
dept_name as bank_name,
case when dept_name like '%农村商业%' or dept_name like '%农商%' then '农商银行'
when dept_name like '%邮储%' or dept_name like '%邮政储蓄%' then '邮储银行'
when dept_name like '%农业%' or dept_name like '%农行%' then '农业银行'
else '其他银行' end as bank_class,
org_id as region_code,
region_name,
date_format(create_dt,'%Y-%m-%d') as reg_dt,
curdate() as create_time,
curdate() as update_time
from (
select  t1.*,t2.dept_name,t2.region_name from 
(
		select mgr_id ,cert_no,tel_no,mgr_name,org_id,create_dt  
		from   dw_base.dwd_mgr_info  
		where dept_name is null 
) t1 left join 
(
    select distinct t1.bank_mgr,t1.county_id,t1.region_name,t3.dept_name from 
    (
				select  distinct t1.bank_mgr,t2.area_id as county_id,loan_bank,concat(t2.city_name,'市',area_name) as region_name
        from dw_base.dwd_guar_info_all t1
				left join 
				(select distinct  *  from  dw_nd.ods_imp_area_map) t2
				on left(t1.city_name,2)=t2.city_name and t1.county_name=t2.county_name
				where t1.bank_mgr is not null
     ) t1
     inner join (select  distinct dept_name  from dw_base.dwd_mgr_info where dept_name is not null ) t3
     on  t1.loan_bank=t3.dept_name
) t2 on t1.mgr_name=t2.bank_mgr and t1.org_id=t2.county_id
order  by length(dept_name) desc
) a  group by mgr_id,mgr_name
union 
select  
t1.mgr_id as bank_mgr_id,
t1.mgr_name as bank_mgr_name,
null as bank_mgr_certno,
null as bank_mgr_telno,
null as bank_name,
null as bank_class,
null as region_code,
null as region_name,
null as reg_dt,
curdate() as create_time,
curdate() as update_time
 from  (
select  distinct mgr_id,mgr_name   from  dw_base.dwd_guar_mgr_info 
where mgr_id is not null and mgr_name is not null 
)t1
left join dw_base.dwd_mgr_info t2
on t1.mgr_id=t2.mgr_id
where t2.mgr_id is null 

</sql>
