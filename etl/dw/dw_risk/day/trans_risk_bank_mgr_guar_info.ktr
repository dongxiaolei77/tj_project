    <connection>dw_risk_risk</connection>
    <sql>select
t1.bank_mgr_id as bank_mgr_id,
t1.bank_mgr_name as bank_mgr_name,
t2.bank_mgr_certno,
t2.bank_mgr_telno as tel_no,
t2.bank_name,
min(coalesce(apply_dt,aprv_dt)) as earliest_create_dt,
max(coalesce(apply_dt,aprv_dt)) as latest_create_dt,
min(loan_dt) as earliest_loan_dt,
max(loan_dt) as latest_loan_dt,
ifnull(count( distinct case when cust_type='自然人' then cert_no end),0) as  per_cust_num,
ifnull(count(distinct case when  cust_type='自然人' then guar_id end),0) as  per_order_num,
ifnull(sum(case when item_stt in ('在保','解保','代偿') and cust_type='自然人' then loan_amt end),0) as  per_fk_amt,
ifnull(count( distinct case when  cust_type='企业' then cert_no end),0) as  comp_cust_num,
ifnull(count(distinct case when cust_type='企业' then guar_id end),0) as  comp_order_num,
ifnull(sum(case when item_stt in ('在保','解保','代偿') and cust_type='企业' then loan_amt end),0) as  comp_fk_amt,
ifnull(count(distinct case when item_stt in ('在审')  then cert_no end),0) as approve_cust_num,
ifnull(count(distinct case when item_stt in ('在审')  then guar_id end),0) as approve_order_num,
ifnull(sum(case when item_stt='在审' then coalesce(apply_amt,loan_amt) end),0) as approve_amt,
ifnull(count(distinct case when item_stt in ('在保')  then cert_no end),0) as mgr_cust_num,
ifnull(count(distinct case when item_stt in ('在保')  then guar_id end),0) as loan_order_num,
ifnull(sum(case when item_stt='在保' then loan_amt end),0) as loan_amt,
ifnull(count(distinct case when item_stt in ('解保','代偿')  then cert_no end),0) as unguar_cust_num,
ifnull(count(distinct case when item_stt in ('解保','代偿')  then guar_id end),0) as unguar_order_num,
ifnull(sum(case when item_stt in ('解保','代偿') then loan_amt end),0) as unguar_amt,
ifnull(count( distinct cert_no),0) as  total_cust_num,
ifnull(count(distinct  guar_id),0) as  total_order_num,
ifnull(sum(case when item_stt in ('在保','解保','代偿') then loan_amt end),0) as  total_fk_amt,
ifnull(count(distinct case when item_stt in ('否决')  then cert_no end),0) as vote_cust_num,
ifnull(count(distinct case when item_stt in ('否决')  then guar_id end),0) as vote_order_num,
ifnull(sum(case when item_stt='否决' then coalesce(apply_amt,loan_amt) end),0) as vote_amt,
ifnull(count(distinct case when item_stt in ('在保') and is_bank_warn=1  then cert_no end),0) as bank_warn_cust_num,
ifnull(count(distinct case when item_stt in ('在保') and is_bank_warn=1  then guar_id end),0) as bank_warn_order_num,
ifnull(sum(case when item_stt='在保' and is_bank_warn=1 then loan_amt end),0) as bank_warn_amt,
ifnull(count( distinct case when item_stt in ('代偿') then cert_no end),0) as  his_dc_cust_num,
ifnull(count(distinct case when item_stt in ('代偿') then guar_id end),0) as  his_dc_order_num,
ifnull(sum(case when item_stt='代偿' then loan_amt end),0) as his_dc_amt,
ifnull(count(distinct case when (back_to_xmtb_num>0) then cert_no end ),0) as back_to_xmtb_cust_num,
ifnull(sum(back_to_xmtb_num),0) as back_to_xmtb_num,
ifnull(count(distinct case when (back_to_xmtb_num>0) then guar_id end ),0) as back_to_xmtb_order_num,
ifnull(count(distinct case when (back_to_bcjd_num>0) then cert_no end ),0) as back_to_bcjd_cust_num,
ifnull(sum(back_to_bcjd_num),0) as back_to_bcjd_num,
ifnull(count(distinct case when (back_to_bcjd_num>0) then guar_id end ),0) as back_to_bcjd_order_num,
ifnull(count(distinct case when (back_to_sgzx_num>0) then cert_no end ),0) as back_to_sgzx_cust_num,
ifnull(sum(back_to_sgzx_num),0) as back_to_sgzx_num,
ifnull(count(distinct case when (back_to_sgzx_num>0) then guar_id end ),0) as back_to_sgzx_order_num,
ifnull(round(sum(case when item_stt='在保' and is_bank_warn=1 and datediff(date_format(curdate(),'%Y-%m-%d'),loan_dt)>=90 then loan_amt end)/sum(case when item_stt in ('在保') and datediff(date_format(curdate(),'%Y-%m-%d'),loan_dt)>=90 then loan_amt end),4),0) as bank_warn_rate,
ifnull(round(sum(case when item_stt='代偿' then loan_amt end)/sum(case when item_stt in ('代偿','解保') then loan_amt end),4),0) as dc_amt_rate,
ifnull(round(count(distinct case when (back_to_xmtb_num>0 or back_to_bcjd_num>0)  then guar_id end )/count(distinct guar_id ),4),0) as back_to_bank_rate,
ifnull(round(count(distinct case when (back_to_sgzx_num>0) then guar_id end )/count(distinct guar_id ),4),0) as back_to_sgzx_rate,
ifnull(round(count(distinct case when item_stt  in ('否决') then guar_id end )/count(distinct case when item_stt&lt;&gt;'在审' then guar_id end ),4),0) as vote_rate,
ifnull(round(count(distinct case when aprv_amt&lt;apply_amt and item_stt&lt;&gt;'在审' then guar_id end )/count(distinct case when item_stt&lt;&gt;'在审' then guar_id end ),4),0) as de_amt_rate,
curdate() as create_time,
curdate() as update_time
from 
dw_risk.risk_guar_prj_info t1
left  join dw_risk.risk_bank_mgr_base_info t2
on t1.bank_mgr_id=t2.bank_mgr_id
where t1.bank_mgr_id is not null and t1.bank_mgr_name &lt;&gt; ''
group by t1.bank_mgr_id,t1.bank_mgr_name,t2.bank_mgr_telno,t2.bank_name,t2.bank_mgr_certno</sql>
