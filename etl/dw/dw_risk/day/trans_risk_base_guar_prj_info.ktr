    <sql>select 
  replace(trim(t1.cert_no),'	','') as cust_id,-- 客户id
  replace(trim(t1.cert_no),'	','') as cert_no,
  case when cust_name='&lt;div>李秀光 &lt;br>&lt;/div>' then '李秀光 ' else trim(t1.cust_name) end as cust_name,
  trim(t1.tel_no) as tel_no,
  case when  t7.code is not null then '自然人' else  '企业' end as cust_type,
  t1.guar_id,-- 项目编号
  date_format(t1.create_dt,'%Y-%m-%d') as apply_dt,
  t1.appl_amt as apply_amt,
  date_format(coalesce( least(create_dt, loan_begin_dt), create_dt, loan_begin_dt),'%Y-%m-%d') as guar_dt,-- 初审日期
  case when item_stt in ('已批复', '待出函', '已出函', '待签约', '已签约', '待放款', '已放款', '已解保','已代偿') then '1'   -- 初审通过
	when item_stt in ('不受理', '已否决', '退审') then '0' -- 初审拒绝
	else '2' end as trial_result, -- 其它
	case when item_stt in ('已放款', '已解保', '已代偿') then '1'
	else '0' end as is_disburse,-- 是否支用
  date_format(t1.aprv_dt,'%Y-%m-%d') as aprv_dt,
  t1.aprv_amt,
  date_format(t1.opera_tm,'%Y-%m-%d') as opera_dt, -- 最后一次系统操作日期
  t6.fk_letter_dt,
  t1.aprv_amt as fk_letter_amt,
  coalesce(coalesce(case when t1.loan_begin_dt='' then null else date_format(t1.loan_begin_dt,'%Y-%m-%d') end ,case when notify_dt='' then null else date_format(notify_dt,'%Y-%m-%d') end ),case when t1.loan_notify_dt='' then null else date_format(t1.loan_notify_dt,'%Y-%m-%d') end )
  as loan_dt,-- 借款日期
  coalesce(date_format(t1.loan_end_dt,'%Y-%m-%d'),case when t1.due_dt='' then null else date_format(t1.due_dt,'%Y-%m-%d') end) as due_dt,-- 责任到期日
  t6.unguar_date as unguar_dt, -- 解保日期
  case when coalesce(loan_amt,guar_amt)>=100000 then coalesce(loan_amt,guar_amt)/10000 else coalesce(loan_amt,guar_amt) end as loan_amt,
  case when t1.is_compensate =1 and t9.seq_id is not null then t9.s_compt_amt else  null end as dc_amt, 
  case when t1.is_compensate =1 and t9.seq_id is not null then date_format(t9.s_compt_dt,'%Y-%m-%d') else null end  as dc_dt,
  t9.s_warn_dt as bank_warn_dt,
  case when t9.is_bank_warn='1' then 1 else 0 end as is_bank_warn,
  case 
  when t2.loan_type='0' then '普通贷款'
  when t2.loan_type='1' then '自主循环贷（随借随还）'
  when t2.loan_type='2' then '非自主循环贷（一年一支用）' else null end as loan_type,
  coalesce(case when t1.loan_term='' then null else t1.loan_term end,case when t1.aprv_term ='' then null else aprv_term end )
  as loan_term,-- 贷款期限
  case when ifnull(t1.loan_use,t1.remark) is null or ifnull(t1.loan_use,t1.remark)='' then null else t1.loan_use end as loan_use,-- 贷款用途
  case when t1.is_first_guar='是' then '1' when t1.is_first_guar='否' then '0' when t1.is_first_guar is null or t1.is_first_guar='' then null else t1.is_first_guar end as is_first_guar,
  case when (t1.item_stt= '已否决' and t1.cur_node_name not in ('项目提报','提报人','签约收费')) or (t1.item_stt='已终止' and aprv_amt=0) then '否决'
  when t1.item_stt in ('待放款','待签约','提报中','审批中','待出函','已出函','已批复','退审') then '在审'
  when t1.item_stt in ('已关闭','手工关闭','不受理','超期终止') then '无效'
  when t1.item_stt='已放款' then '在保' when t1.item_stt='已解保' and t1.is_compensate=0 then '解保' when t1.item_stt='已代偿' then '代偿' else '终止' end as item_stt,-- 农担项目状态
  case when t1.item_stt= '已否决' and t1.cur_node_name not in ('项目提报','提报人','签约收费') then '否决1' when  t1.item_stt='已终止' and aprv_amt=0 then '否决2'
  when t1.item_stt in ('待放款','待签约','提报中','审批中','待出函','已出函','已批复','退审','已关闭','手工关闭','不受理','超期终止') then t1.item_stt
  when t1.item_stt='已放款' then '在保' when t1.item_stt='已解保' and t1.is_compensate=0 then '解保' when t1.item_stt='已代偿'  then '代偿' else '终止' end as item_branch,-- 农担项目状态
  t1.cur_node_name,
  case when t8.back_to_xmtb in ('0','1') then 0 else t8.back_to_xmtb-1 end +
  case when back_to_tbr in ('0','1') then 0 else t8.back_to_tbr-1 end  back_to_xmtb_num,
  t8.back_to_bcjd  as back_to_bcjd_num,
  t8.back_to_sgzx  as back_to_sgzx_num,
  t2.mgr_id as bank_mgr_id,
  t3.schema_code as clus_scheme_id,
  t5.code as guar_indus_id,
  concat(left(t4.area_id,4),'00') as city_id,
  t4.area_id as county_id,
  null as district_id,
  t4.area_id as region_id, -- 没有村级的行政代码，暂时用区县代替
  t6.handles_bank as bank_id,
  case when t2.mgr_name='' or mgr_name is null then null else trim(t2.mgr_name) end as bank_mgr_name,-- 银行客户经理姓名
  case when t3.new_schema='' or t3.new_schema is null then null else t3.new_schema end as clus_scheme_name,-- 产业集群名称
  case when t5.type is null then null else t5.type end as guar_indus_name,
  case when t4.area_id is null then null else concat(t4.city_name,'市',t4.area_name) end as region_name, -- 所属地区名称（区县）
  replace(trim(t1.loan_bank),' ','') as bank_name,
  t1.guar_prod,
  curdate() as create_time,
  curdate() as update_time
 from dw_base.dwd_guar_info_all t1
 left join 
(
	select  t1.guar_id,t2.mgr_id,t1.mgr_name,t1.loan_type from 
	(
		select  t1.guar_id,t1.mgr_id,t1.mgr_name,t1.loan_type,t3.area_id  
		from  dw_base.dwd_guar_mgr_info t1 
		left join dw_base.dwd_guar_info_all t2
		on t1.guar_id=t2.guar_id 
		left join (select distinct  *  from  dw_nd.ods_imp_area_map) t3
 		on left(t2.city_name,2)=t3.city_name and t2.county_name=t3.county_name
		where t1.mgr_id is null  
	) t1 left   join 
	(
		select   *   from  dw_base.dwd_mgr_info  where org_id is not null
		group by mgr_name,org_id having count(*)=1 
	) t2 on t1.mgr_name=t2.mgr_name and t1.area_id=t2.org_id
union 
select guar_id,mgr_id,mgr_name,loan_type from dw_base.dwd_guar_mgr_info 
where mgr_id is not null 
) t2
 on t1.guar_id=t2.guar_id
 left join ( select distinct  * from  dw_nd.ods_guar_schema_mapping ) t3 
 on t1.clus_scheme=t3.old_schema
 left join (select distinct  *  from  dw_nd.ods_imp_area_map) t4
 on left(t1.city_name,2)=t4.city_name and t1.county_name=t4.county_name
 left join (select distinct type,code  from dw_nd.ods_imp_sys_gmjjfl) t5
on t1.econ_class=t5.type
left join 
(
		select  
		distinct t1.code,
        t1.handles_bank,
		date_format(t2.unguar_date,'%Y-%m-%d') as  unguar_date,
		date_format(t3.fk_letter_date,'%Y-%m-%d') as  fk_letter_dt
		from  
		(   
        select distinct code,id,handles_bank from (
				select   distinct id,code,handles_bank
				from dw_nd.ods_t_biz_project_main 
				where code is not null  order by update_time desc) a group by code
		) t1
		left join 
		(
        select  *  from  (
				select distinct project_id,unguar_date  
				from dw_nd.ods_t_biz_proj_unguar
        where project_id is not null 
        order by update_time desc ) a group by project_id 
     ) t2
		on t1.id=t2.project_id
		left  join 
		(   
				select *  from  (
				select  distinct project_id,fk_letter_date 
				from dw_nd.ods_t_biz_proj_loan
				where project_id is not null 
				order by update_time desc) a group by project_id
		) t3
		on t1.id=t3.project_id
) t6 on t1.guar_id=t6.code
left join 
(select   distinct left(province_code,2) as code from  dw_base.dim_chinese_district_code) t7
on left(trim(t1.cert_no),2)=t7.code
left join 
(
   select 
   proj_no,
   sum(case when task_node in ('grt_xmtb','grt_xz_xztb') then 1 else 0 end ) as back_to_xmtb,
   sum(case when task_node in ('grt_tbr') then 1 else 0 end ) as back_to_tbr,
   sum(case when task_node in ('grt_tbrbczl','grt_xz_bcjd') then 1 else 0 end ) as back_to_bcjd,
   sum(case when task_node in ('grt_sglzx','grt_xz_sglzx') then 1 else 0 end ) as back_to_sgzx
   from  dw_base.dwd_evt_wf_task_info 
   where proj_no is not null group by proj_no 
) t8 on t1.guar_id=t8.proj_no
left  join  
(
		select  seq_id,date_format(s_warn_dt,'%Y-%m-%d') as s_warn_dt,
    s_compt_amt,date_format(s_compt_dt,'%Y-%m-%d') as s_compt_dt,
    case when s_risk_stt='预警' then 1 else 0 end as is_bank_warn  
    from dw_nd.ods_imp_portrait_info_new 
    where s_risk_stt in ('预警','已代偿')
) t9 on t1.guar_id=t9.seq_id
where item_stt is not null and t1.guar_id is not null
-- and t1.cert_no ='370829198209195336' 
order by loan_dt  </sql>
