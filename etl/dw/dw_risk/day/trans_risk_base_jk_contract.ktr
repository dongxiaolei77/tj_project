    <sql>select  
t1.loan_cont_id as jk_contract_code, -- 借款合同编号
t1.proj_no as guar_id,-- 主项目编号
t1.is_sign_max_guar, -- 是否签订最高额保证合同
t2.loan_bank as loan_bank_name, -- 贷款银行名称
case when t2.loan_bank like '%农村商业%' or t2.loan_bank like '%农商%' then '农商银行'
when t2.loan_bank like '%邮储%' or t2.loan_bank like '%邮政储蓄%' then '邮储银行'
when t2.loan_bank like '%农业%' or t2.loan_bank like '%农行%' then '农业银行'
else '其他银行' end as bank_class, -- 贷款银行分类
trim(t2.cust_name) as cust_name,-- 贷款主体名称
t2.cust_type,-- 贷款主体类型
replace(trim(t2.cert_no),'	','') as cert_no,-- 证件号码
trim(t2.tel_no) as tel_no,-- 联系方式
t1.loan_cont_beg_dt as jk_contract_begin_dt,-- 借款合同开始日期
t1.loan_cont_end_dt as jk_contract_end_dt,-- 借款合同结束日期
t1.loan_cont_term as jk_contract_terms,-- 借款期限（月）
t1.loan_cont_rate as jk_contract_rate, -- 利率
round(t1.loan_cont_amt,2) as jk_contract_amt, -- 借款合同金额（万元）
case 
  when t1.loan_type='0' then '普通贷款'
  when t1.loan_type='1' then '自主循环贷（随借随还）'
  when t1.loan_type='2' then '非自主循环贷（一年一支用）' 
else null end as debt_type,-- 贷款方式
t2.loan_use as debt_use, -- 贷款用途
date_format(t2.loan_begin_dt,'%Y-%m-%d') as first_fk_dt,-- 合同内首笔放款日期
round(coalesce(case when t2.guar_amt>10000 then t2.guar_amt/10000 else t2.guar_amt end ,loan_amt),2)  as first_fk_amt, -- 合同内首笔放款金额
t2.data_source,-- 数据来源
t2.guar_prod, -- 担保产品
curdate() as create_time,
curdate() as update_time
from 
(
	select   distinct t1.proj_no,t1.loan_type,t2.*  
	from  dw_base.tmp_ods_t_biz_project_main t1
	inner join  dw_base.tmp_ods_t_biz_proj_sign t2 
	on t1.proj_id=t2.proj_id
) t1 left join 
(select  *   from dw_base.dwd_guar_info_all) t2
on t1.proj_no=t2.guar_id
where t2.item_stt in('已放款','已解保')
union
select
t1.loan_no as jk_contract_code,
t1.guar_id,
null as is_sign_max_guar,
t1.loan_bank as loan_bank_name,
case when t1.loan_bank like '%农村商业%' or t1.loan_bank like '%农商%' then '农商银行'
when t1.loan_bank like '%邮储%' or t1.loan_bank like '%邮政储蓄%' then '邮储银行'
when t1.loan_bank like '%农业%' or t1.loan_bank like '%农行%' then '农业银行'
else '其他银行' end as bank_class, 
trim(t1.cust_name) as cust_name,-- 贷款主体名称
t1.cust_type,-- 贷款主体类型
replace(trim(t1.cert_no),'	','') as cert_no,-- 证件号码
trim(t1.tel_no) as tel_no,-- 联系方式
t1.loan_begin_dt as jk_contract_begin_dt,
t1.loan_end_dt as jk_contract_end_dt,
t1.loan_term as jk_contract_terms,
t1.loan_rate as jk_contract_rate,
t1.loan_amt as jk_contract_amt,
case 
  when t3.loan_type='0' then '普通贷款'
  when t3.loan_type='1' then '自主循环贷（随借随还）'
  when t3.loan_type='2' then '非自主循环贷（一年一支用）' 
else null end as debt_type,-- 贷款方式
t1.loan_use as debt_use,
t1.loan_begin_dt as first_fk_dt,
t1.loan_amt as first_fk_amt,
t1.data_source,
t1.guar_prod,
curdate() as create_time,
curdate() as update_time
from 
(
	select  *   from dw_base.dwd_guar_info_all 
	where item_stt in ('已放款','已解保') and guar_id not like '%XZ%'
	group by replace(trim(cert_no),'	','') 
	having count(guar_id)=1
)  t1
left join 
(
	select   distinct t1.proj_no,t1.loan_type,t2.*  
	from  dw_base.tmp_ods_t_biz_project_main t1
	inner join  dw_base.tmp_ods_t_biz_proj_sign t2 
	on t1.proj_id=t2.proj_id
) t2 
on t1.guar_id=t2.proj_no
left join dw_base.tmp_ods_t_biz_project_main  t3
on t1.guar_id=t3.proj_no
where t2.proj_no is null
union 
-- 按照证件号码和合同号分组，取多个值去重
select
replace(trim(t1.loan_no),'	','') as jk_contract_code,
min(t1.guar_id) as guar_id,
null as is_sign_max_guar,
t1.loan_bank as loan_bank_name,
case when t1.loan_bank like '%农村商业%' or t1.loan_bank like '%农商%' then '农商银行'
when t1.loan_bank like '%邮储%' or t1.loan_bank like '%邮政储蓄%' then '邮储银行'
when t1.loan_bank like '%农业%' or t1.loan_bank like '%农行%' then '农业银行'
else '其他银行' end as bank_class, 
trim(t1.cust_name) as cust_name,-- 贷款主体名称
t1.cust_type,-- 贷款主体类型
replace(trim(t1.cert_no),'	','') as cert_no,-- 证件号码
trim(t1.tel_no) as tel_no,-- 联系方式
date_format(min(t1.loan_begin_dt),'%Y-%m-%d') as jk_contract_begin_dt,
date_format(max(t1.loan_end_dt),'%Y-%m-%d') as jk_contract_end_dt,
case when max(t1.loan_term)= 12 then sum(loan_term) else max(t1.loan_term) end as jk_contract_terms,
max(t1.loan_rate) as jk_contract_rate,
max(t1.loan_amt) as jk_contract_amt,
null as debt_type,-- 贷款方式
t1.loan_use as debt_use,
date_format(min(t1.loan_begin_dt),'%Y-%m-%d') as first_fk_dt,
min(t1.loan_amt) as first_fk_amt,
t1.data_source,
t1.guar_prod,
curdate() as create_time,
curdate() as update_time
from 
( 
	select t1.*
  from dw_base.dwd_guar_info_all t1
  inner join 
	(
		select  distinct replace(trim(cert_no),'	','') as cert_no,replace(trim(loan_no),'	','') as loan_no   
    from dw_base.dwd_guar_info_all 
		where  loan_no is not null and loan_no&lt;&gt;'' and loan_no&lt;&gt;'无'
    and guar_id not like '%XZ%' and item_stt in ('已放款','已解保') 
		group by replace(trim(cert_no),'	','') ,replace(trim(loan_no),'	','')
		having count(guar_id)>1
	) t2
   on replace(trim(t1.cert_no),'	','')=t2.cert_no and replace(trim(t1.loan_no),'	','')=t2.loan_no
   where guar_id not like '%XZ%' and item_stt in ('已放款','已解保') 
  
)  t1
left join 
(
	select   distinct t2.loan_cont_id as loan_no
	from  dw_base.tmp_ods_t_biz_project_main t1
	inner join  dw_base.tmp_ods_t_biz_proj_sign t2 
	on t1.proj_id=t2.proj_id
) t2 
on t1.loan_no=t2.loan_no
where t2.loan_no is null 
group by replace(trim(t1.cert_no),'	','') ,replace(trim(t1.loan_no),'	','')

union 
-- 按照证件号码和合同号码分组，取唯一值
select
replace(trim(t1.loan_no),'	','') as jk_contract_code,
t1.guar_id as guar_id,
null as is_sign_max_guar,
t1.loan_bank as loan_bank_name,
case when t1.loan_bank like '%农村商业%' or t1.loan_bank like '%农商%' then '农商银行'
when t1.loan_bank like '%邮储%' or t1.loan_bank like '%邮政储蓄%' then '邮储银行'
when t1.loan_bank like '%农业%' or t1.loan_bank like '%农行%' then '农业银行'
else '其他银行' end as bank_class, 
trim(t1.cust_name) as cust_name,-- 贷款主体名称
t1.cust_type,-- 贷款主体类型
replace(trim(t1.cert_no),'	','') as cert_no,-- 证件号码
trim(t1.tel_no) as tel_no,-- 联系方式
date_format(t1.loan_begin_dt,'%Y-%m-%d') as jk_contract_begin_dt,
date_format(t1.loan_end_dt,'%Y-%m-%d') as jk_contract_end_dt,
t1.loan_term  as jk_contract_terms,
t1.loan_rate as jk_contract_rate,
t1.loan_amt as jk_contract_amt,
null as debt_type,-- 贷款方式
t1.loan_use as debt_use,
date_format(t1.loan_begin_dt,'%Y-%m-%d') as first_fk_dt,
t1.loan_amt as first_fk_amt,
t1.data_source,
t1.guar_prod,
curdate() as create_time,
curdate() as update_time
from 
( 
	select *  
  from dw_base.dwd_guar_info_all 
	where  loan_no is not null and loan_no&lt;&gt;'' and loan_no&lt;&gt;'无' and loan_no not like '1%' and loan_no not like '0%'
  and guar_id not like '%XZ%' and item_stt in ('已放款','已解保') 
	group by replace(trim(cert_no),'	','') ,replace(trim(loan_no),'	','')
	having count(guar_id)=1
)  t1
left join 
(
	select   distinct t2.loan_cont_id as loan_no
	from  dw_base.tmp_ods_t_biz_project_main t1
	inner join  dw_base.tmp_ods_t_biz_proj_sign t2 
	on t1.proj_id=t2.proj_id
) t2 
on t1.loan_no=t2.loan_no
left join 
(
	select  *   from dw_base.dwd_guar_info_all 
	where item_stt in ('已放款','已解保') and guar_id not like '%XZ%'
	group by replace(trim(cert_no),'	','') 
	having count(guar_id)=1
) t3
on t1.guar_id=t3.guar_id
where t2.loan_no is null and t3.guar_id is null </sql>
