    <connection>dw_base</connection>
    <sql>select  cert_no as obj_id,cert_no  as obj_certno, final_result, group_concat(distinct refuse_name) as refuse_dtl ,curdate() as create_time,curdate() as update_time
	from 
	(
		select  t1.*,t2.refuse_name
		from  
		(
		select  distinct cert_no,final_result,substring_index(substring_index(a.reject_code,',',b.help_topic_id+1),',',-1) as reject_list
		from (
				select
				cust_id_no as cert_no,  
				case when final_result in ('0','001') then '拒绝'
				when final_result='1' then '通过'
				when final_result in('002','003') then '转人工'
				when final_result='004' then '转法人' end as final_result,
				case when reject_code is null then '无' else reject_code end as reject_code
				from 
				(
					select *  from  dw_nd.ods_t_sdnd_credit_log  
					where final_result in ('001','002','003','004','0','1')  
					and cust_id_no is not null  and length(cust_id_no)=18 and msg_code in ('RIM10290','RIM10291','RIM00800','RIM00801','RIM01002') order by query_time desc) a 
					group by a.cust_id_no
				) a
				join dw_nd.dim_help_topic b 
				on b.help_topic_id &lt; (length(a.reject_code) - length(replace(a.reject_code,',',''))+1)
		) t1 
		left join
		(select *  from  dw_base.dim_d_credit_refuse ) t2
		on t1.reject_list=t2.reject_code 
	) a 
	group by cert_no;</sql>
