    <connection>dw_base</connection>
    <sql>select 
cert_no as cust_id,
cust_name,
cert_no,
case when cust_type='法人或其他组织' then '企业' else cust_type end as cust_type,
is_reject_forever,
replace(group_concat(risk_reason),',','&amp;')  as reject_reason,
'1' as  reject_sts,
min(begin_dt) as begin_dt, 
max(end_dt) as end_dt,
curdate() as create_time,
curdate() as update_time
from  
(
		select cert_no,cust_name,cust_type,is_reject_forever,risk_reason,min(begin_dt) as begin_dt,max(end_dt) as end_dt
    from dw_base.dwd_cust_prj_risk_list
		where  risk_type='拒绝' and date_format(curdate(),'%Y-%m-%d')&lt;=end_dt 
    group by cert_no,risk_reason
		order by cert_no,risk_reason
) a
group by cert_no
order by cert_no</sql>
