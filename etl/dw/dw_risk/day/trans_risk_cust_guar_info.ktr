  <step>
    <name>表输入 2</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>dw_base</connection>
    <sql>select  cert_no ,final_result, group_concat(refuse_name) as refuse_dtl,query_time from 
(
select  t1.*,t2.refuse_name
	from  
	(
	select  distinct cert_no,final_result,substring_index(substring_index(a.reject_code,',',b.help_topic_id+1),',',-1) as reject_list,query_time
	from (
				select
				cust_id_no as cert_no,  
				case when final_result in ('0','001') then '拒绝'
				when final_result='1' then '通过'
				when final_result in('002','003') then '转人工'
				when final_result='004' then '转法人' else '预警' end as final_result,
				reject_code,
				date_format(query_time,'%Y-%m-%d') as query_time
				from 
				(
					select *  from  dw_nd.ods_t_sdnd_credit_log 
					where  msg_code not in ('RIM02002','RIM02001','RIM05101','RIM05102','RIM05103','RIM05104','RIM05105','RIM05106','RIM05107')
					and cust_id_no is not null  and length(cust_id_no)=18  order by query_time desc) a 				
			group by a.cust_id_no
				) a
	join dw_nd.dim_help_topic b 
	on b.help_topic_id &lt; (length(a.reject_code) - length(replace(a.reject_code,',',''))+1)
) t1 
left join
(select *  from  dw_base.dim_d_credit_refuse)
t2
on t1.reject_list=t2.reject_code ) a 
group by cert_no,final_result
order by cert_no
</sql>
