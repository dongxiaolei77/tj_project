    <connection>dw_risk_risk</connection>
    <sql>select
	t1.cert_no  as cust_id,
    t1.cust_name,
	t1.cert_no,
	cust_type,
    case when group_concat(distinct item_stt)= '解保' then 1 else 0 end as is_jb,
	case when group_concat(item_stt) like '%在保%'  then 1 else 0 end as is_zb,
	case when group_concat(item_stt) like '%在审%'  then 1 else 0 end as is_zs,
	ifnull(sum(case when item_stt= '在保' then loan_amt end),0) as zb_amt,
	ifnull(sum(case when item_stt= '在审' then loan_amt end),0) as zs_amt,
    max(case when item_stt='否决' then opera_dt else null end) as last_vote_dt,
    curdate() as create_time,
    curdate() as update_time
from  risk_guar_prj_info t1
left join (select  cert_no,ls_max_yq_days   from  dw_risk.risk_cust_overdue_info) t2
on t1.cert_no=t2.cert_no
left join 
(select  distinct cert_no from  risk_guar_prj_info where item_stt='代偿' or is_bank_warn='1') t3
on t1.cert_no=t3.cert_no
where t3.cert_no is null 
group by t1.cust_name,t1.cert_no,t1.cust_type
order by t1.cert_no</sql>
    <limit>0</limit>
