    <sql>select  *  from  (
select 

clus_scheme,
group_concat(distinct schema_code) scheme_code,

-- 已解保业务统计
max(case when (item_stt in ('已解保','已放款','审批中','已出函','待出函','待放款','待签约','提报中')) then loan_begin_dt end) recent_loan_dt,
min(case when (item_stt in ('已解保','已放款','审批中','已出函','待出函','待放款','待签约','提报中')) then loan_begin_dt end) early_loan_dt,
-- 无效业务统计
count(distinct case when (item_stt in ('不受理','已关闭','已终止','手工关闭')) then guar_id end) invalid_guar_num,
sum(case when (item_stt in ('不受理','已关闭','已终止','手工关闭')) then appl_amt end) invalid_apply_amt,
-- 退回业务统计
count(distinct case when (item_stt in ('已否决','退审') or item_stt is null) then guar_id end) reject_guar_num,
sum(case when (item_stt in ('已否决','退审') or item_stt is null) then appl_amt end) reject_apply_amt,
-- 已解保业务统计
count(distinct case when item_stt in ('已解保') then guar_id end ) end_guar_num,
sum(case when item_stt in ('已解保') then loan_amt end ) end_guar_amt,
-- 在保业务统计
count(distinct case when item_stt in ('已放款') then guar_id end ) under_guar_num,
sum(case when item_stt in ('已放款') then loan_amt end ) under_guar_amt,
-- 审批中待放款业务统计
count(distinct case when item_stt in ('审批中','待放款','提报中','待签约','待出函','已批复','已出函') then guar_id end ) under_aprv_guar_num,
sum(case when item_stt in ('审批中','待放款','提报中','待签约','待出函','已批复','已出函') then appl_amt end ) under_aprv_guar_amt,

-- 代偿业务统计
count(distinct case when s_risk_stt = '已代偿' then guar_id end) compt_num,
sum(case when s_risk_stt = '已代偿' then s_compt_amt end) compt_amt,

-- 预警业务统计
count(distinct case when s_risk_stt = '预警' then guar_id end) warn_num,
sum(case when s_risk_stt = '预警' then loan_conteact_amount end) warn_amt,

-- 逾期业务统计
count(distinct vf_overdue.cert_no) vf_ovdue_num,
sum(yq_amt) vf_ovdue_amt

from 
(

select  
guar_id,
cert_no,
scheme_map.new_schema clus_scheme,
case when scheme_map.old_schema is not null then scheme_map.schema_code end as schema_code,
dim_scheme.scheme_type,
dim_scheme.scheme_chain,
dim_scheme.scheme_product,

loan_begin_dt,
item_stt,
cust_type,
loan_amt,
appl_amt

from 
dw_base.dwd_guar_info_all a
left join 
-- select * 
dw_nd.ods_guar_schema_mapping  scheme_map on a.clus_scheme = scheme_map.old_schema

left join 
dw_base.dim_d_clus_scheme dim_scheme on scheme_map.schema_code = dim_scheme.scheme_code

-- limit 100
where 

cert_no is not null 
and cert_no&lt;&gt;'' 
and guar_id is not null 
and guar_id &lt;&gt; ''
and clus_scheme is not null 
and clus_scheme &lt;&gt; ''
) guar left join 


(
############# 表二 代偿和银行预警客户
select 
id_number cert_no,loan_begin_date,item_status,
s_risk_stt,
s_compt_amt,
s_compt_dt,
loan_conteact_amount,
s_warn_dt,s_ovdue_dt
from 
dw_nd.ods_imp_portrait_info_new 
where
s_risk_stt in 
('预警','已代偿')
) bank_bad on guar.cert_no = bank_bad.cert_no
left join 

(
############# 表三 潍坊联社逾期客户，当前逾期情况
select 
cert_no,
capt_yq_amt + int_yq_amt yq_amt,
yq_days
from 
dw_base.dwd_cust_yq_info
where yq_days > 0
) vf_overdue on guar.cert_no = vf_overdue.cert_no

group by 1
) a where scheme_code is not null </sql>
