    <sql>select 
distinct 
a.*,
curdate() as create_time,
curdate() as update_time
from (
select 
distinct  
per_cust_id as per_obj_id,
per_cust_name as per_obj_name, 
age,
cert_no,
tel_no
from  dw_base.dwd_cust_per_base_info
union 
select * from 
(
select  
distinct 
counter_guar_certno as per_obj_id, 
counter_guar_name as  per_obj_name, 
round(datediff(curdate(), date_format(substring(counter_guar_certno,7,8),'%Y%m%d'))/365.2422) as age,
counter_guar_certno as cert_no,
ct_guar_person_phone as tel_no
from  dw_base.dwd_counter_guar_info 
where counter_guar_type='自然人' and counter_guar_certno not in (select distinct cert_no from dw_base.dwd_cust_per_base_info) 
order by tel_no desc
) a group by per_obj_id,per_obj_name
-- and counter_guar_certno not in ('370723196309025569')
union 
select  *  from  (
select
distinct   
legal_certno as per_obj_id,
legal_name as  per_obj_name,
round(datediff(curdate(), date_format(substring(legal_certno,7,8),'%Y%m%d'))/365.2422) as age,
legal_certno as cert_no,
legal_telno as tel_no
from  dw_base.dwd_cust_comp_base_info where legal_certno is not null and legal_name is not null 
union 
select 
legal_person_certno as per_obj_id,
legal_person_name as legal_name,
round(datediff(curdate(), date_format(substring(legal_person_certno,7,8),'%Y%m%d'))/365.2422) as age,
legal_person_certno as cert_no,
null as tel_no
from  dw_base.dwd_counter_guar_info where legal_person_certno is not null and legal_person_name is not null 
) a  
where per_obj_id not in 
(select 
distinct  
per_cust_id
from  dw_base.dwd_cust_per_base_info
union 
select  
distinct counter_guar_certno as per_obj_id
from  dw_base.dwd_counter_guar_info 
where counter_guar_type='自然人' and counter_guar_certno not in (select distinct cert_no from dw_base.dwd_cust_per_base_info)
-- and counter_guar_certno not in ('370723196309025569')
) group by per_obj_id
) a </sql>
